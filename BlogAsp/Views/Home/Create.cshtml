@model BlogAsp.Models.User

@{
    ViewData["Title"] = "Добавление пользователя";
}
<form asp-controller="Home" asp-action="Create">
    <div class="form-group">
        <input type="email" asp-for="Email" placeholder="Enter email" class="form-control">
    </div>
    <div class="form-group">
        <input type="text" asp-for="Login" placeholder="Enter login" class="form-control">
    </div>
    <div class="form-group">
        <input type="password" asp-for="Password" placeholder="Enter password" class="form-control">
    </div>
    <div class="form-group">
        <input type="submit" value="Добавить" class="form-control">
    </div>
</form>


